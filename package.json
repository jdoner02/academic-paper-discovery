    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "d3": "^7.8.5",
    "lodash": "^4.17.21",
    "next": "^14.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^6.1.0",
