{
  "name": "research-paper-aggregator",
  "version": "0.1.0",
  "description": "Comprehensive research paper aggregation and discovery platform with batch processing, visual concept mapping, and Clean Architecture implementation",
  "private": false,
  "scripts": {
    "dev": "cd frontend && next dev",
    "build": "cd frontend && next build",
