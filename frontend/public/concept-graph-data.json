{
  "nodes": [
    {
      "id": "predicate_logic",
      "name": "Predicate Logic First-Order Logic",
      "type": "definition",
      "domain": "logic",
      "subdomain": "predicate_logic",
      "level": "undergraduate",
      "hierarchy_level": 2,
      "parent_domain": "foundations.logic",
      "formal_statement": "An extension of propositional logic that includes quantifiers $\\forall$ (for all) and $\\exists$ (there exists), predicates $P(x)$, and variables $x$ that can range over objects in a domain.",
      "informal_description": "A more powerful form of logic that can express statements about properties of objects and relationships between them, using phrases like 'for all' and 'there exists'.",
      "examples": [
        {
          "description": "Universal quantification",
          "formal": "$\\forall x \\in \\mathbb{N}, x + 0 = x$",
          "explanation": "For every natural number x, adding zero gives x back",
          "level": "high_school"
        },
        {
          "description": "Existential quantification",
          "formal": "$\\exists x \\in \\mathbb{R}, x^2 = 2$",
          "explanation": "There exists a real number whose square is 2",
          "level": "high_school"
        },
        {
          "description": "Mixed quantifiers",
          "formal": "$\\forall \\epsilon > 0, \\exists \\delta > 0, |x - a| < \\delta \\rightarrow |f(x) - f(a)| < \\epsilon$",
          "explanation": "The formal definition of continuity in calculus",
          "level": "undergraduate"
        }
      ],
      "difficulty": 6,
      "group": "logic",
      "size": 26,
      "color": "#4ECDC4"
    },
    {
      "id": "propositional_logic",
      "name": "Propositional Logic",
      "type": "definition",
      "domain": "logic",
      "subdomain": "propositional_logic",
      "level": "high_school",
      "hierarchy_level": 2,
      "parent_domain": "foundations.logic",
      "formal_statement": "A formal system dealing with propositions that can be true or false, combined using logical connectives: $\\neg$ (negation), $\\land$ (conjunction), $\\lor$ (disjunction), $\\rightarrow$ (implication), $\\leftrightarrow$ (biconditional).",
      "informal_description": "The foundation of logical reasoning using statements that are either true or false, combined with words like 'and', 'or', 'not', and 'if-then'.",
      "examples": [
        {
          "description": "Simple proposition",
          "formal": "$P$: 'It is raining'",
          "explanation": "A basic statement that can be either true or false",
          "level": "elementary"
        },
        {
          "description": "Conjunction",
          "formal": "$P \\land Q$: 'It is raining AND it is cold'",
          "explanation": "Both statements must be true for the whole statement to be true",
          "level": "middle_school"
        },
        {
          "description": "Implication",
          "formal": "$P \\rightarrow Q$: 'If it is raining, then the ground is wet'",
          "explanation": "When the first part is true, the second part must also be true",
          "level": "high_school"
        }
      ],
      "difficulty": 3,
      "group": "logic",
      "size": 21,
      "color": "#4ECDC4"
    },
    {
      "id": "zfc_axiom_empty_set",
      "name": "Axiom of Empty Set",
      "type": "axiom",
      "domain": "set_theory",
      "subdomain": "zfc_axioms",
      "level": "undergraduate",
      "hierarchy_level": 3,
      "parent_domain": "foundations.set_theory.axioms",
      "formal_statement": "$\\exists A \\forall x (x \\notin A)$",
      "informal_description": "There exists a set that contains no elements at all. This set is called the empty set and is usually denoted âˆ… or {}.",
      "examples": [
        {
          "description": "The empty set",
          "formal": "$\\emptyset = \\{\\}$",
          "explanation": "A set with no elements inside",
          "level": "middle_school"
        },
        {
          "description": "Set of unicorns",
          "formal": "$\\{x : x \\text{ is a unicorn}\\} = \\emptyset$",
          "explanation": "Since unicorns don't exist, this set has no elements",
          "level": "high_school"
        },
        {
          "description": "Solutions to impossible equation",
          "formal": "$\\{x \\in \\mathbb{R} : x^2 = -1\\} = \\emptyset$",
          "explanation": "No real number squared gives -1, so this set is empty",
          "level": "high_school"
        }
      ],
      "difficulty": 4,
      "group": "set_theory",
      "size": 28,
      "color": "#FF6B6B"
    },
    {
      "id": "zfc_axiom_extensionality",
      "name": "Axiom of Extensionality",
      "type": "axiom",
      "domain": "set_theory",
      "subdomain": "zfc_axioms",
      "level": "undergraduate",
      "hierarchy_level": 3,
      "parent_domain": "foundations.set_theory.axioms",
      "formal_statement": "$\\forall A \\forall B (\\forall x (x \\in A \\leftrightarrow x \\in B) \\rightarrow A = B)$",
      "informal_description": "Two sets are equal if and only if they have exactly the same elements. A set is completely determined by its members, not by how it's described or constructed.",
      "examples": [
        {
          "description": "Equal sets with different descriptions",
          "formal": "$\\{x \\in \\mathbb{N} : x < 3\\} = \\{0, 1, 2\\}$",
          "explanation": "Both describe the same set containing 0, 1, and 2",
          "level": "high_school"
        },
        {
          "description": "Order doesn't matter",
          "formal": "$\\{1, 2, 3\\} = \\{3, 1, 2\\} = \\{2, 3, 1\\}$",
          "explanation": "These are all the same set because they contain exactly the same elements",
          "level": "middle_school"
        },
        {
          "description": "Repetition doesn't matter",
          "formal": "$\\{1, 2, 2, 3\\} = \\{1, 2, 3\\}$",
          "explanation": "A set either contains an element or it doesn't - no duplicates",
          "level": "middle_school"
        }
      ],
      "difficulty": 5,
      "group": "set_theory",
      "size": 28,
      "color": "#FF6B6B"
    },
    {
      "id": "zfc_axiom_pairing",
      "name": "Axiom of Pairing",
      "type": "axiom",
      "domain": "set_theory",
      "subdomain": "zfc_axioms",
      "level": "undergraduate",
      "hierarchy_level": 3,
      "parent_domain": "foundations.set_theory.axioms",
      "formal_statement": "$\\forall a \\forall b \\exists A \\forall x (x \\in A \\leftrightarrow x = a \\lor x = b)$",
      "informal_description": "For any two objects a and b, there exists a set that contains exactly those two objects (and nothing else). This allows us to form pairs, and by extension, finite sets.",
      "examples": [
        {
          "description": "Pair of numbers",
          "formal": "$\\{2, 5\\}$",
          "explanation": "A set containing exactly the numbers 2 and 5",
          "level": "elementary"
        },
        {
          "description": "Pair with identical elements",
          "formal": "$\\{3, 3\\} = \\{3\\}$",
          "explanation": "When both elements are the same, we get a singleton set",
          "level": "middle_school"
        },
        {
          "description": "Pair of sets",
          "formal": "$\\{\\{1, 2\\}, \\{3, 4\\}\\}$",
          "explanation": "Sets can themselves be elements of pairs",
          "level": "high_school"
        }
      ],
      "difficulty": 5,
      "group": "set_theory",
      "size": 30,
      "color": "#FF6B6B"
    },
    {
      "id": "zfc_axiom_union",
      "name": "Axiom of Union",
      "type": "axiom",
      "domain": "set_theory",
      "subdomain": "zfc_axioms",
      "level": "undergraduate",
      "hierarchy_level": 3,
      "parent_domain": "foundations.set_theory.axioms",
      "formal_statement": "$\\forall A \\exists B \\forall x (x \\in B \\leftrightarrow \\exists Y (Y \\in A \\land x \\in Y))$",
      "informal_description": "For any set A (a collection of sets), there exists a set B that contains exactly those elements that belong to at least one set in A. This is the union of all sets in A.",
      "examples": [
        {
          "description": "Union of two sets",
          "formal": "$\\{1, 2\\} \\cup \\{2, 3\\} = \\{1, 2, 3\\}$",
          "explanation": "The union contains elements that are in either set",
          "level": "middle_school"
        },
        {
          "description": "Union of a family of sets",
          "formal": "$\\bigcup \\{\\{1\\}, \\{1, 2\\}, \\{1, 2, 3\\}\\} = \\{1, 2, 3\\}$",
          "explanation": "Take all elements that appear in any of the sets in the collection",
          "level": "high_school"
        },
        {
          "description": "Union of empty collection",
          "formal": "$\\bigcup \\emptyset = \\emptyset$",
          "explanation": "Union of no sets is the empty set",
          "level": "undergraduate"
        }
      ],
      "difficulty": 6,
      "group": "set_theory",
      "size": 30,
      "color": "#FF6B6B"
    },
    {
      "id": "set_membership_concept",
      "name": "Set Membership Relation",
      "type": "definition",
      "domain": "set_theory",
      "subdomain": "basic_concepts",
      "level": "middle_school",
      "hierarchy_level": 3,
      "parent_domain": "foundations.set_theory.basic_concepts",
      "formal_statement": "The relation $x \\in A$ read as 'x is an element of A' or 'x belongs to A' is the fundamental primitive relation of set theory.",
      "informal_description": "The basic relationship between an object and a collection (set) that contains it. Like saying 'this apple is in this basket' or 'this student is in this class'.",
      "examples": [
        {
          "description": "Number in a set",
          "formal": "$3 \\in \\{1, 2, 3, 4\\}$",
          "explanation": "The number 3 is an element of the set containing 1, 2, 3, and 4",
          "level": "elementary"
        },
        {
          "description": "Letter in alphabet set",
          "formal": "$'a' \\in \\{a, b, c, d, e\\}$",
          "explanation": "The letter 'a' belongs to the set of first five letters",
          "level": "elementary"
        },
        {
          "description": "Element not in set",
          "formal": "$5 \\notin \\{1, 2, 3, 4\\}$",
          "explanation": "The number 5 is NOT an element of this set",
          "level": "elementary"
        },
        {
          "description": "Set as element",
          "formal": "$\\{1, 2\\} \\in \\{\\{1, 2\\}, \\{3, 4\\}\\}$",
          "explanation": "Sets can themselves be elements of other sets",
          "level": "high_school"
        }
      ],
      "difficulty": 2,
      "group": "set_theory",
      "size": 23,
      "color": "#FF6B6B"
    }
  ],
